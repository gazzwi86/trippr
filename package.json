{
  "name": "trippr",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:server": "NODE_ENV=development ENV=local nodemon ./server",
    "start:prod": "NODE_ENV=development ENV=local nodemon ./build/server",
    "build": "npm run build:react && npm run build:server",
    "build:react": "react-scripts build",
    "build:server": "cp -r ./server ./build/server && cp ./.env* ./build/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "devDependencies": {
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.24.0",
    "mongoose": "^5.4.6",
    "morgan": "^1.9.1",
    "newrelic": "^5.2.0",
    "nodemon": "^1.18.9",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "rotating-file-stream": "^1.4.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
